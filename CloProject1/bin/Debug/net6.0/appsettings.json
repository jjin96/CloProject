{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Log4NetCore": {
    "PropertyOverrides": [
      {
        "XPath": "/log4net/root/Debug",
        "Attributes": {
          "value": "Debug"
        }
      },
      {
        "XPath": "/log4net/root/Info",
        "Attributes": {
          "value": "Info"
        }
      },
      {
        "XPath": "/log4net/root/Error",
        "Attributes": {
          "value": "Error"
        }
      }
    ]
  },
  "AppSettings": {
    "JsonPath": "Employee.json",
    "CsvPath": "Employee.csv"
  },
  "AllowedHosts": "*"
}
